<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane id="Pane" prefHeight="651.0" prefWidth="200.0" style="-fx-background-color: #24574f;" stylesheets="@DaycareCSS.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="samplefx.LoginViewFXMLController">
   <children>
      <VBox prefHeight="651.0" prefWidth="200.0">
         <children>
            <Label prefHeight="250.0" prefWidth="200.0" />
            <Button fx:id="parentLoginBtn" alignment="BASELINE_LEFT" graphicTextGap="10.0" mnemonicParsing="false" onAction="#parentLogin" prefHeight="60.0" prefWidth="194.0" text="Parent Login" textFill="#fcf6f6" underline="true">
               <graphic>
                  <FontAwesomeIcon fill="#fcf9f9" glyphName="CHECK" size="20" />
               </graphic>
               <padding>
                  <Insets left="40.0" />
               </padding>
            </Button>
            <Button fx:id="teacherLoginBtn" alignment="BASELINE_LEFT" graphicTextGap="10.0" mnemonicParsing="false" onAction="#teacherLogin" prefHeight="60.0" prefWidth="194.0" text="Teacher Login" textFill="#fcf6f6" underline="true">
               <graphic>
                  <FontAwesomeIcon fill="#fcf9f9" glyphName="BOOK" size="20" />
               </graphic>
               <padding>
                  <Insets left="40.0" />
               </padding>
            </Button>
         </children>
      </VBox>
   </children>
</Pane>
